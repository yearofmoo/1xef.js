src:
  input:
    - Source/files/xef.Base.js
    - Source/files/xef.Assets.js
    - Source/files/xef.Page.js
    - Source/files/xef.Page.Request.js
  output:
    - Source/xef.js

jsmin:
  filters:
    - cat Source/xef.js | jsmin > %{output}
  input:
    - Source/xef.js
  output:
    - Source/xef.min.js
